{
  "name": "mithril-redux-starter-webpack",
  "version": "0.1.3",
  "description": "Mithril Redux + Webpack",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch": "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test": "babel-node spec/support/test.js"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "baggage-loader": "^0.2.4",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch": "^1.0.0",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "html-loader": "^0.4.0",
    "image-webpack-loader": "^1.6.3",
    "imports-loader": "^0.6.5",
    "jasmine": "^2.4.1",
    "jasmine-expect": "^1.22.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.2.0",
    "lost": "^6.7.1",
    "mithril": "^0.2.2-rc.1",
    "postcss": "^5.0.14",
    "postcss-bem": "^0.4.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-extend": "^1.0.1",
    "postcss-functions": "^2.1.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "redux": "^3.2.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.5.1",
    "rucksack-css": "^0.8.5",
    "sanitize.css": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
