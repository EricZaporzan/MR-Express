{
  "name": "mithril-redux-starter-webpack",
  "version": "0.1.2",
  "description": "Mithril Redux + Webpack",
  "config": {
    "buildDir": "./build"
  },
  "scripts": {
    "watch": "webpack-dev-server --content-base ./build/ --inline --hot --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir"
  },
  "devDependencies": {
    "babel-core": "^5.1.11",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.0.0",
    "babel-plugin-mjsx": "^1.0.3",
    "babel-runtime": "^5.1.11",
    "baggage-loader": "^0.2.4",
    "css-loader": "^0.23.0",
    "es6-promise": "^3.0.2",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "fetch": "^0.3.6",
    "file-loader": "^0.8.1",
    "font-awesome": "^4.4.0",
    "html-loader": "^0.4.0",
    "image-webpack-loader": "^1.6.2",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.2",
    "lodash": "^3.3.1",
    "lost": "^6.6.2",
    "mithril": "^0.2.0",
    "postcss": "^5.0.12",
    "postcss-bem": "^0.4.0",
    "postcss-cssnext": "^2.2.0",
    "postcss-extend": "^1.0.1",
    "postcss-functions": "^2.0.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.3.2",
    "rucksack-css": "^0.8.5",
    "sanitize.css": "^3.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.5",
    "webpack-dev-server": "^1.8.2"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
